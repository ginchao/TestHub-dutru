local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))() 
 local Window = OrionLib:MakeWindow({Name = "Key System", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"}) 
 local plr = game.Players.LocalPlayer 
 local Tab = Window:MakeTab({Name = "",Icon = "",PremiumOnly = false}) 
 function keya() 
 OrionLib:MakeNotification({Name = "Welcome To Test Hub", Content = "Hello!".." "..plr.Name, Image = "rbxassetid://4483345998", Time = 10}) 
 end 
 function keyf() 
 OrionLib:MakeNotification({Name = "Warning", Content = "Hello!".." "..plr.Name.." ".."Your Key incorrect", Image = "rbxassetid://4483345998", Time = 4}) 
 end 
 local Key = loadstring(game:HttpGet("https://raw.githubusercontent.com/ginchao/HTT/main/Key"))() 
 getgenv().Keyin = "" 
  
 Tab:AddTextbox({Name = "Private Key",Default = "",Callback = function(v) 
 getgenv().Keyin = v 
 end     
 }) 
 Tab:AddButton({Name = "Check Key",Callback = function() 
 if getgenv().Keyin == Key then 
 keya() 
 wait() loadstring(game:HttpGet('https://raw.githubusercontent.com/ginchao/TestHub1/Universal/Credit%20X'))()
 else 
 keyf() 
 end 
 end 
 })
